  },
  "dependencies": {
    "@nuxt/ui": "3.1.3",
    "@nuxtjs/tailwindcss": "7.0.0-beta.0",
    "@pinia/nuxt": "^0.11.1",
    "@vueuse/nuxt": "13.4.0",
    "nuxt": "3.17.5",
    "pinia": "^3.0.3",
    "typescript": "5.8.3",
    "vue": "3.5.17",
    "vue-router": "4.5.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.52",
    "bumpp": "latest",
    "prettier": "^3.6.1",
    "wrangler": "^4.21.2"
  },
  "packageManager": "pnpm@10.12.3",
  "engines": {
    "node": ">=v20.10.0"
  }
