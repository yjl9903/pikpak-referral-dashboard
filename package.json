{
  "name": "pikpak-referral",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "cloudflare:build": "nuxt build --preset=cloudflare_pages",
    "cloudflare:deploy": "wrangler --cwd dist/ pages deploy",
    "dev": "nuxt dev",
    "format": "prettier --write ./**/*.{vue,ts}",
